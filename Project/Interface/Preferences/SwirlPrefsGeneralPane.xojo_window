	#tag Method, Flags = &h21
		Private Sub UpdateUserNick(nick As Text)
		  Prefs.UserNick = nick
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub UpdateUserStatus(status As Text)
		  Prefs.UserStatus = status
		End Sub
