#tag Events PushButton1
	#tag Event
		Sub Action()
		  DIM f As FolderItem = SelectFolder
		  if (f <> Nil) AND (f.Exists) then
		    self.UserDownloadFolder.Text = f.NativePath
		    Prefs.DownloadFolder = NEW xojo.IO.FolderItem(f.NativePath.ToText)
		  end if
		End Sub
